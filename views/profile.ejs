<div id="container" class="container">
    <div style="color:red" id="error"></div>
    <div class="container-narrow">
      <div class="masthead">
        <ul class="nav nav-pills pull-right">
          <li class="active"><a href="#">Home</a></li>
        </ul>
        <h3 class="muted">Welcome to WebApp</h3>
      </div>
      <hr>
      <div class="pull-right form-signout">
  <form class="form-logout" id="form-logout" action='/user/logout' method="post">
    <button class="btn btn-small btn-primary logout-btn" id="logoutbtn" type="submit">Log Out</button>
  </form>
</div>

      <div class="jumbotron">
        <h1>Congrats you have successfully logged in</h1>
        <p class="lead">Now follow your friends and their daily statuses</p>
      </div>

      <div class ="masthead" id="user-follow">
      </div>
      <hr>
<div class="row-fluid marketinfunctiong">
        <div class="span7">
          <h4>Your thoughts for Today</h4>
        </div>
      </div>

<div id="status_div">
<div style="" class="pull-right form-signin">
<% if(session.result) {%>
<% for(var i=0; i < session.result.length; i++) { %>
    <div><% var person = session.result[i] %></div>
    <div>Name: <%= person.firstname %> 
    <form name='follow-form' id='follow-form' method='post' action='/user/follow'>
    <input class='input-small' type='hidden' name='followUserId' value='<%= person.id %>' >
    <button class="btn btn-small btn-primary follow-btn" align="center" type="submit">Follow me</button>
    </form>
    </div>
<% } %>
<%} %>
</div>
  <div class="form-inline">upload-status-btn
  <form name='update-status-form' id='upload-status-form' method='post' action='/user/updateStatus'>
    <input class="input-xxlarge" type="text" placeholder="Please put your status here." id="textId" name='uploadData'>
    <button class="btn btn-primary" id="upload-status-btn" align="center" type="submit">Upload</button>
  </form>
  </div>
  <hr>


  <div class="pull-left">
<% if(session.message != 'empty') {%>
<% for(var i=0; i < session.message.length; i++) { %>
 <% var message = session.message[i] %></div>
 <div class="media">
              <a href="#" class="pull-left">
                <img data-src="holder.js/64x64" class="media-object" alt="64x64" style="width: 64px; height: 64px;" src="/img/photo.png">
              </a>
              <div class="media-body">
                <h4 class="media-heading"><%=message.firstname%></h4>
                <%=message.message%>
              </div>
  </div>
    
<% } %>
<% } %>
  </div>
</div>

<div style="clear:both"></div>
</div>

</div>
<hr>
<div class="footer">
  <p>&copy; Company 2012</p>
</div>
</div> <!-- /container -->

